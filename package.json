{
  "name": "marcusolsson-dynamictext-panel",
  "version": "1.9.0-custom",
  "description": "A panel plugin for Grafana for dynamic, data-driven text",
  "keywords": [
    "grafana"
  ],
  "license": "Apache-2.0",
  "scripts": {
    "build": "grafana-toolkit plugin:build",
    "test": "grafana-toolkit plugin:test",
    "dev": "grafana-toolkit plugin:dev",
    "watch": "grafana-toolkit plugin:dev --watch",
    "sign": "grafana-toolkit plugin:sign",
    "lint": "prettier --check src",
    "lint:fix": "prettier --write src",
    "package": "yarn build && mv dist marcusolsson-dynamictext-panel && rm -f marcusolsson-dynamictext-panel-1.9.0-custom.zip && zip marcusolsson-dynamictext-panel-1.9.0-custom.zip marcusolsson-dynamictext-panel -r && rm -rf marcusolsson-dynamictext-panel"
  },
  "homepage": "https://grafana.com/plugins/marcusolsson-dynamictext-panel",
  "bugs": "https://github.com/marcusolsson/grafana-dynamictext-panel/issues",
  "repository": "github:marcusolsson/grafana-dynamictext-panel",
  "author": {
    "name": "Marcus Olsson",
    "email": "marcus.olsson@hey.com",
    "url": "https://marcus.se.net"
  },
  "dependencies": {
    "handlebars": "^4.7.7",
    "helper-date": "^1.0.1",
    "markdown-it": "^12.0.4",
    "rison": "^0.1.1",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@emotion/core": "10.0.27",
    "@grafana/data": "8.2.7",
    "@grafana/runtime": "8.2.7",
    "@grafana/toolkit": "8.2.7",
    "@grafana/ui": "8.2.7",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@types/markdown-it": "^12.0.1",
    "@types/rison": "^0.0.7",
    "emotion": "10.0.27",
    "prettier": "^2.7.1"
  },
  "engines": {
    "node": ">=14"
  }
}
